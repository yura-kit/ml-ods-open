    "def variance(y):\n",
    "    '''\n",
    "    $F(X) = \\dfrac{1}{|X|} \\sum_{x_j \\in X}(y_j - \\dfrac{1}{|X|}\\sum_{x_i \\in X}y_i)^2$\n",
    "    '''\n",
    "    l = len(y)\n",
    "    if l == 0:\n",
    "        return 0\n",
    "    mean = (1/l)*np.sum(y)\n",
    "    s = 0\n",
    "    for i in range(l):\n",
    "        s += (y[i]-mean)**2\n",
    "    return (1/l)*s\n",
    "\n",
    "def mad_median(y):\n",
    "    l = len(y)\n",
    "    med = np.median(y)\n",
    "    s = 0\n",
    "    for i in range(l):\n",
    "        s += np.abs(y[i]-med)\n",
    "    return (1/l)*s\n"
